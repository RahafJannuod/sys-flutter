# Development Environment Configuration
APP_NAME=Community Marketplace (Dev)
APP_ENV=development
DEBUG=true

# API Configuration
BASE_API_URL=https://dev-api.communitymarketplace.com
WEBSOCKET_URL=wss://dev-ws.communitymarketplace.com
API_TIMEOUT=30000

# Firebase Configuration (Development)
FIREBASE_PROJECT_ID=community-marketplace-dev
FIREBASE_API_KEY=your-dev-firebase-api-key
FIREBASE_AUTH_DOMAIN=community-marketplace-dev.firebaseapp.com
FIREBASE_DATABASE_URL=https://community-marketplace-dev-default-rtdb.firebaseio.com
FIREBASE_STORAGE_BUCKET=community-marketplace-dev.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:web:development-app-id

# Google Sign-In Configuration (Development)
GOOGLE_WEB_CLIENT_ID=your-dev-google-web-client-id.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=your-dev-google-ios-client-id.googleusercontent.com
GOOGLE_ANDROID_CLIENT_ID=your-dev-google-android-client-id.googleusercontent.com

# Map Configuration
GOOGLE_MAPS_API_KEY=your-development-google-maps-api-key

# Push Notifications
FCM_SERVER_KEY=your-dev-fcm-server-key
FCM_SENDER_ID=123456789

# Payment Configuration (Test Mode)
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_key
STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret

# Analytics
GOOGLE_ANALYTICS_ID=GA-DEV-XXXXXXXXX

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DARK_MODE=true
ENABLE_CACHING=true

# Performance Settings
MAX_IMAGE_SIZE=5242880
CACHE_EXPIRATION_HOURS=24
MAX_CACHE_SIZE=104857600
NETWORK_TIMEOUT=30
SHORT_TIMEOUT=10

# Location Settings (Damascus, Syria as default for development)
MAX_SEARCH_RADIUS=50.0
DEFAULT_LATITUDE=33.5138
DEFAULT_LONGITUDE=36.2765

# Pagination
PRODUCT_LIST_PAGE_SIZE=20
CHAT_MESSAGE_LIMIT=100
